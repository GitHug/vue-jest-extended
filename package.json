{
  "name": "vue-jest-extended",
  "version": "0.0.1",
  "description": "Jest helper matchers compatible with Vue-Test-Utils",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "yarn lint && yarn cover",
    "test:prod": "cross-env BABEL_ENV=production yarn test",
    "test:only": "NODE_ENV=test jest --no-cache",
    "test:watch": "yarn test --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage yarn test:only",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "yarn clean && yarn lint && yarn test && yarn build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GitHug/vue-jest-extended.git"
  },
  "keywords": [
    "jest",
    "extend",
    "extended",
    "vue",
    "vue-test-utils"
  ],
  "author": {
    "name": "Fredrik Mäkilä",
    "email": "fredrik@takanoha.io",
    "url": "https://takanoha.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GitHug/vue-jest-extended/issues"
  },
  "homepage": "https://github.com/GitHug/vue-jest-extended#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-minify": "^0.5.0",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^22.13.5",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "jest": "^24.8.0",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "rootDir": "test",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
