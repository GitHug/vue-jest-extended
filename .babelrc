{
  "env": {
    "development": {
      "presets": [
        "@babel/preset-env"
      ],
      "plugins": [
        "add-module-exports",
        "@babel/plugin-transform-modules-commonjs"
      ]
    },
    "test": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs"
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-env",
        "minify"
      ],
      "plugins": [
        "add-module-exports",
        "@babel/plugin-proposal-object-rest-spread"
      ]
    }
  }
}
